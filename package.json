{
  "name": "@rossmacd/gamesock-server",
  "version": "0.0.1",
  "description": "A Serverside Networking library to handle the websocket portion of games",
  "main": "dist/server.js",
  "types": "./dist/server.d.ts",
  "scripts": {
    "test": "jest --silent --detectOpenHandles",
